<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
        <style>
            .navbar{
                margin-top: 25px;
                margin-bottom: 50px;
                background-color: #0A0747 !important; align-content: center;
            }
            .navbar-nav{
                align-content: center;
            }
            a.nav-link{
                color: white!important;
            }
            .dropdown-menu{
                background-color: #333f4f !important;
                color: white;
                padding-top: 1em!important;
            }
            #navbarNav {
                position: center;
                left: 0px;
                height: 40px;
                width: 100%;
                text-align: center;
            }
            #navbarNav ul {
                display: flex;
            }
            .card-subtitle{
                background-color: #003151!important;
                color: white!important;
                margin-bottom: 25px!important;
                margin-top: 25px!important;
                height: 70px;
                text-align: center;
                padding-top: 15px!important;
            }
            .dropdown-menu{
                margin-top: 11px !important;
            }
            .dropdown-item{
                color: white;
            }
            body{
                background-color: white;
            }
            #header{
                position: sticky;
                top: 0;
                z-index: 999999999999999999999999999999;
            }

        </style>
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}


    </head>
    <body>
    {# Menu #}
    <header id="header">
        {% block menu %}
        {% endblock %}
    </header>
    <main role="main" id="content">
        <div id="main-fluid-pre" class="container-fluid">
            {% block prebody %}{% endblock %}
        </div>
        <div id="main" class="container">
            {% block body %}{% endblock %}
        </div>
        <div id="main-fluid-post" class="container-fluid">
            {% block postbody %}{% endblock %}
        </div>
    </main>


    {# Footer #}
    <footer >

    </footer>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js">
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            $(".js-example-basic-single").select2();
        });
    </script>

    {% block javascripts %}
        {#{{ encore_entry_script_tags('app') }}#}
    {% endblock %}
    </body>

</html>
